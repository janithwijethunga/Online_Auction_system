<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	version="3.1">


	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.employee.Loginservlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/log</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>employeeinsert</servlet-name>
		<servlet-class>com.employee.employeeinsert</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>employeeinsert</servlet-name>
		<url-pattern>/insert</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>contact</servlet-name>
		<servlet-class>com.contactus.ContactServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>contact</servlet-name>
		<url-pattern>/insertc</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>feed</servlet-name>
		<servlet-class>com.feedbck.Feedservlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>feed</servlet-name>
		<url-pattern>/feed</url-pattern>
	</servlet-mapping>
	
	


</web-app>